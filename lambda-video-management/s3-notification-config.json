{
  "LambdaFunctionConfigurations": [
    {
      "Id": "VideoUploadAutoConvert",
      "LambdaFunctionArn": "arn:aws:lambda:ap-northeast-1:730335478220:function:video_management",
      "Events": [
        "s3:ObjectCreated:*"
      ],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "prefix",
              "Value": "videos/"
            },
            {
              "Name": "suffix",
              "Value": ".mp4"
            }
          ]
        }
      }
    }
  ]
}